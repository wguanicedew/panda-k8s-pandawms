{
    "mb_servers": {
        "iDDS_mb": {
            "host_port_list": ["${PANDA_ACTIVEMQ_LIST}"],
            "use_ssl": false,
            "cert_file": null,
            "key_file": null,
            "username": "jedi",
            "passcode": "${PANDA_ACTIVEMQ_PASSWD_jedi}",
            "verbose": true
        },
        "internal_mb": {
            "host_port_list": ["${PANDA_ACTIVEMQ_LIST}"],
            "use_ssl": false,
            "cert_file": null,
            "key_file": null,
            "username": "jedi",
            "passcode": "${PANDA_ACTIVEMQ_PASSWD_jedi}",
            "verbose": true
        }
    },
    "queues": {
        "idds": {
            "server": "iDDS_mb",
            "destination": "/queue/iddspanda"
        },
        "jedi_taskstatus": {
            "server": "internal_mb",
            "destination": "/queue/self.jedi_taskstatus"
        },
        "to-idds": {
            "server": "iDDS_mb",
            "destination": "/queue/pandaidds"
        },
        "panda_jedi": {
            "server": "internal_mb",
            "destination": "/queue/panda_jedi"
        }
    },
    "processors": {
        "atlas-idds": {
            "module": "pandajedi.jedimsgprocessor.atlas_idds_msg_processor",
            "name": "AtlasIddsMsgProcPlugin",
            "in_queue": "idds",
            "verbose": true
        },
        "push_task_status": {
            "module": "pandajedi.jedimsgprocessor.forwarding_msg_processor",
            "name": "ForwardingMsgProcPlugin",
            "in_queue": "jedi_taskstatus",
            "out_queue": "to-idds",
            "verbose": true
        },
        "proc_msg_from_panda": {
            "module": "pandajedi.jedimsgprocessor.panda_to_jedi_msg_processor",
            "name": "PandaToJediMsgProcPlugin",
            "in_queue": "panda_jedi",
            "verbose": true
        }
    }
}
